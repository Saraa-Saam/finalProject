{
  "usesTypeScript": false,
  "usesStyledComponents": false,
  "usesCssModule": true,
  "cssPreprocessor": "css",
  "testLibrary": "None",
  "component": {
    "default": {
      "customTemplates": {
        "component": "Template/TemplateName.jsx",
        "style": "Template/TemplateName.module.css"
      },
      "path": "src/components",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "page": {
      "path": "src/pages",
      "customTemplates": {
        "component": "Template/TemplateName.jsx",
        "style": "Template/TemplateName.module.css"
      },

      "withStyle": true
    }
  }
}